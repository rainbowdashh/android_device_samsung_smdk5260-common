import init.universal5260.usb.rc
import init.wifi.rc
import init.target.rc

on init
#    export LD_SHIM_LIBS /system/bin/gpsd|libdmitry.so

    mkdir /efs 0771 radio system
    mkdir /preload 0771 system system

 # Support legacy paths
    symlink /sdcard /storage/sdcard0
    symlink /sdcard1 /storage/sdcard1

    symlink /dev/block/mmcblk0p13 /dev/mbin0
    restorecon /dev/mbin0
    symlink  /sys/devices/platform/s5p-ehci/ehci_power /dev/ehci_power

# Vibetonz
    export VIBE_PIPE_PATH /dev/pipes
    mkdir /dev/pipes 0771 vibe vibe
    restorecon /dev/pipes

# Make param link
    symlink /dev/block/mmcblk0p8 /dev/block/param

on post-fs
     # create camera directory
     mkdir /data/vendor/camera 0770 camera camera

     # camera server uses files under this directory via a kernel module for the FIMC, trying chmod since chown not successful
     chown cameraserver audio /data/misc/media
     chmod 0776 /data/misc/media

on post-fs-data

    setprop vold.post_fs_data_done 1

    # Added for TZ Playready DRM Support
    mkdir /efs/drm 0774 drm system
    mkdir /efs/drm/playready 0775 drm system

    # Multimedia property for Smart View
    setprop media.enable-commonsource true

    # For kernel lock in BT/WiFi race condtion
    chmod 0600 /dev/btlock
    chown bluetooth bluetooth /dev/btlock

# YAAS(Yamaha Android Audio System) uses.
    mkdir /data/ymc 0775 system system

on boot

# Permissions for bluetooth
    setprop ro.bt.bdaddr_path "/efs/bluetooth/bt_addr"
    chown bluetooth ro.bt.bdaddr_path
    chown bluetooth /dev/ttySAC3
    chmod 0660 /dev/ttySAC3
    chmod 0660 /sys/class/rfkill/rfkill0/state
    chown bluetooth /sys/class/rfkill/rfkill0/state
    chown bluetooth /sys/class/rfkill/rfkill0/type

# cmdline 
    chmod 0644 /proc/cmdline

    # Reduce reserved lowmem from ~48MB to ~12MB
    write /proc/sys/vm/lowmem_reserve_ratio "128 128"

    # Set up kernel tracing, but disable it by default
    chmod 0222 /sys/kernel/debug/tracing/trace_marker
    write /sys/kernel/debug/tracing/tracing_on 0
    
# mobicore
    mkdir /data/app/mcRegistry 0775 system system
    chmod 0700 /dev/mobicore
    chmod 0666 /dev/mobicore-user
    chmod 0660 /dev/s5p-smem    
    chown system system /dev/mobicore
    chown radio system /dev/mobicore-user
    chown system system /dev/s5p-smem       

